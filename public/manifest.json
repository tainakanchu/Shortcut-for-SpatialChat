{
  "manifest_version": 3,
  "name": "Shortcut for SpatialChat",
  "description": "Extend SpatialChat's on-screen operation with shortcut keys.",
  "version": "0.0.1",
  "action": {
    "default_icon": {
      "19": "icon_19.png",
      "1024": "icon_1024.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://app.spatial.chat/*"],
      "js": ["js/content_script.js"]
    }
  ]
}
